{
  "name": "clap-server",
  "version": "1.0.0",
  "description": "Server for the CLAP, GraphQL, TypeScript, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/dl0312/Clap-backend",
  "author": "dl0312 <leegun2003@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/cors": "^2.8.4",
    "@types/helmet": "^0.0.38",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mailgun-js": "^0.16.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/twilio": "^0.0.9"
  },
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cd src && copy ./api/**/*.graphql ../dist/api",
    "start": "cd dist && node ."
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "apollo-server": "^2.0.4",
    "apollo-upload-server": "^7.0.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "class-validator": "^0.8.5",
    "copy": "^0.3.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express-graphql": "^0.6.12",
    "fs": "^0.0.1-security",
    "gql-merge": "^0.0.6",
    "graphql-server-express": "^1.4.0",
    "graphql-to-typescript": "^0.1.2",
    "graphql-tools": "^3.0.5",
    "graphql-yoga": "^1.14.12",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "mailgun-js": "^0.20.0",
    "merge-graphql-schemas": "^1.5.2",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.2",
    "pg": "^7.4.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "twilio": "^3.18.0",
    "typeorm": "^0.2.8",
    "typescript": "^3.1.0-dev.20180804",
    "uuid": "^3.3.2"
  },
  "now": {
    "name": "Clap-backend",
    "dotenv": ".env.production"
  }
}
